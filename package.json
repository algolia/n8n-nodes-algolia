{
	"name": "n8n-nodes-algolia",
	"version": "0.1.0",
	"description": "Algolia nodes for n8n",
	"keywords": [
		"n8n-community-node-package",
		"algolia",
		"n8n"
	],
	"license": "MIT",
	"homepage": "https://github.com/algolia/n8n-nodes-algolia",
	"author": {
		"name": "Algolia",
		"email": "support@algolia.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/algolia/n8n-nodes-algolia.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm run build && pnpm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/AlgoliaApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Algolia/Algolia.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"concurrently": "^9.2.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"fdir": "^6.4.6",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"shelljs": "^0.10.0",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.8.2",
		"vite": "^7.0.6",
		"vite-plugin-static-copy": "^3.1.1"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
