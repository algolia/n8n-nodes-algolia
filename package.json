{
  "name": "@algolia/n8n-nodes-algolia",
  "version": "0.11.1",
  "description": "Algolia nodes for n8n",
  "keywords": [
    "n8n-community-node-package",
    "algolia",
    "n8n"
  ],
  "license": "MIT",
  "homepage": "https://github.com/algolia/n8n-nodes-algolia",
  "author": {
    "name": "Algolia",
    "email": "support@algolia.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algolia/n8n-nodes-algolia.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "format": "prettier nodes credentials --write",
    "lint": "n8n-node lint",
    "lintfix": "n8n-node lint --fix",
    "generate-properties": "npx tsx scripts/generate-properties.ts",
    "generate-specs-index": "npx tsx scripts/generate-specs-index.ts",
    "generate-readme-operations": "npx tsx scripts/generate-readme-operations.ts",
    "generate": "npm run generate-properties && npm run generate-specs-index && npm run generate-readme-operations"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AlgoliaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Algolia/Algolia.node.js"
    ]
  },
  "devDependencies": {
    "@algolia/n8n-openapi-node": "^1.3.3",
    "@n8n/node-cli": "^0.13.0",
    "@typescript-eslint/parser": "~8.32.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.38.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "openapi-types": "12.1.3",
    "prettier": "^3.5.3",
    "shelljs": "^0.10.0",
    "shipjs": "^0.27.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.8.2",
    "vite": "^7.0.6",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.107.0"
  }
}
